{
	"extends": ["tslint:recommended", "tslint-config-prettier"],
	"linterOptions": {
	  "exclude": ["node_modules/**/*."]
	},
	"rules": {
	"no-shadowed-variable": false,
	  "no-console": false,
	  "member-access": false,
	  "object-literal-sort-keys": false,
	  "ordered-imports": true,
	  "interface-name": false,
	  "strict-null-checks": false
	},
	"rulesDirectory": []
  }
